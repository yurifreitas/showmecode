{
    "manifest_version": 3,
    "name": "Class meet manager",
    "version": "1.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
      },
    
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "tabGroups",
        "storage",
        "activeTab",
        "tabs",
        "scripting", 
        "webNavigation"
    ]
}